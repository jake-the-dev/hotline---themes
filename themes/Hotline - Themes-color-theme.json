{
  // Theme Color reference.
  // https://code.visualstudio.com/docs/getstarted/theme-color-reference
  "$schema": "vscode://schemas/color-theme",
  "name": "Hotline - Themes",
  "type": "dark",
  "colors": {
    // Base Colors.
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_base-colors
    // Foreground color for description text providing additional information, for example for a label.
    "focusBorder": "#111111",
    "foreground": "#ffffffcc",
    "disabledForeground": "#ffffffcc",
    "widget.border": "#5DD8E6",
    // "widget.shadow": "#5DD8E6",
    "descriptionForeground": "#ffffffcc",
    "selection.background": "#FDE80650",
    // "window.activeBorder": "#FDE806",
    // "window.inactiveBorder": "#5DD8E6",

    // ~~~~~~~ ACTIVITY BAR ~~~~~~~
    // The sidest of bars
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_activity-bar
    "activityBar.background": "#111111",
    "activityBar.border": "#111111",
    "activityBar.dropBorder": "#111111",
    "activityBar.foreground": "#FDE806",
    "activityBar.inactiveForeground": "#ffffffcc",
    "activityBarBadge.background": "#FDE806",
    "activityBarBadge.foreground": "#111111",
    "activityBar.activeBorder": "#F06BAA",
    "activityBar.activeBackground": "#111111",

    // ~~~~~~~ SIDEBAR ~~~~~~~
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_side-bar
    "sideBar.background": "#111111",
    "sideBar.border": "#ffffff05",
    "sideBar.foreground": "#ffffffcc",
    "sideBarTitle.foreground": "#ffffffcc",
    "sideBarSectionHeader.background": "#111111",
    "sideBarSectionHeader.foreground": "#FDE806",
    "sideBarSectionHeader.border": "#111111",

    // Badge. ie. search results (matches)
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_badge
    "badge.background": "#FDE806",
    "badge.foreground": "#111111",

    // Button.
    // ie Sync Changes
    // https: //code.visualstudio.com/docs/getstarted/theme-color-reference#_button-control
    "button.background": "#FDE806dd",
    "button.foreground": "#111111",
    "button.hoverBackground": "#FDE806",
    "button.secondaryBackground": "#FDE806dd",
    "button.secondaryForeground": "#111111",
    "button.secondaryHoverBackground": "#fde806",
    "checkbox.background": "#111111",
    "checkbox.foreground": "#5DD8E6",
    "checkbox.border": "#5DD8E6",

    // Contrast.
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_contrast-colors
    "contrastActiveBorder": "#FFFFFF00",
    "contrastBorder": "#FFFFFF00",

    // Dropdown : ie Output -> Ruby language server
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_dropdown-control
    "dropdown.background": "#111111",
    "dropdown.border": "#111111",
    "dropdown.foreground": "#fde806",

    // ~~~~~~~ EDITOR ~~~~~~~
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_editor-colors

    // --- Gutter ---: The gutter contains the glyph margins and the line numbers:
    "editorGutter.background": "#111111",
    "editorGutter.addedBackground": "#77E8B9",
    "editorGutter.deletedBackground": "#E76057",
    "editorGutter.modifiedBackground": "#F06BAA",
    "editorLineNumber.foreground": "#ffffff30", // Color of editor line numbers.
    "editorLineNumber.activeForeground": "#fde806",

    // --- Editor Cursor ---
    "editorCursor.foreground": "#fde806", //  Color of the editor cursor.

    "editor.background": "#111111", // Editor background color.
    "editor.foreground": "#ffffffcc", // Editor default foreground color.

    // --- Highlight Selection Match ---
    "editor.selectionBackground": "#fde80690", // Color of the editor selection.
    "editor.selectionForeground": "#111111",
    // "editor.inactiveSelectionBackground": "#5DD8E6", // Color of the selection in an inactive editor.
    "editor.selectionHighlightBackground": "#11111190", // Color for regions with the same content as the selection.
    "editor.selectionHighlightBorder": "#fde80660",
    "editor.linkedEditingBackground": "#F06BAA", // HTML Matching Elements.
    // "editor.wordHighlightBackground": "#E76057", // Same words other places.
    "editor.wordHighlightStrongBackground": "#FFFFFF0D", // Background color of a symbol during write-access, for example when writing to a variable
    "editor.findMatchBackground": "#F06BAA70", // Color of the current search match.
    "editor.findMatchHighlightBackground": "#F06BAA40", // Color of the other search matches.
    "editor.findRangeHighlightBackground": "#FFFF0336", // Color the range limiting the search (Enable 'Find in Selection' in the find widget).
    "editor.hoverHighlightBackground": "#11111190", // Changed for co-pilot suggestions. Highlight below the word for which a hover is shown.
    // "editor.lineHighlightBackground": "#E76057", // Current line of code. Background color for the highlight of line at the cursor position.
    "editor.lineHighlightBorder": "#111111",
    "editor.rangeHighlightBackground": "#111111",
    "editorLink.activeForeground": "#5DD8E6",
    "editorIndentGuide.background1": "#111111",
    "editorIndentGuide.activeBackground1": "#5DD8E650",
    "editorRuler.foreground": "#5DD8E61C", // Editor Ruler.
    "editorOverviewRuler.border": "#5DD8E61C",
    "editorCodeLens.foreground": "#5DD8E6",
    "editorBracketMatch.background": "#F06BAA46",
    "editorBracketMatch.border": "#FDE80655",
    "editorBracketHighlight.foreground1": "#5DD8E6",
    "editorBracketHighlight.foreground2": "#FDE806",
    "editorBracketHighlight.foreground3": "#F06BAA",
    "editorBracketHighlight.foreground4": "#77E8B9",
    "editorBracketHighlight.foreground5": "#ffffff",

    // Overview ruler is located beneath the scroll bar on the right edge of the editor and gives an overview of the decorations in the editor.
    "editorOverviewRuler.commonContentForeground": "#FDE80655",
    "editorOverviewRuler.currentContentForeground": "#E7605755",
    "editorOverviewRuler.incomingContentForeground": "#77E8B955",

    // Errors and warnings:
    // "editorError.border": "#E76057",
    "editorError.foreground": "#E76057",
    "editorWarning.border": "#FFFFFF00",
    "editorWarning.foreground": "#FDE806",

    // Diff Editor.
    // https: //code.visualstudio.com/docs/getstarted/theme-color-reference#_diff-editor-colors
    "diffEditor.insertedTextBackground": "#77E8B90E",
    "diffEditor.insertedTextBorder": "#77E8B99A",
    "diffEditor.removedTextBackground": "#E760571A",
    "diffEditor.removedTextBorder": "#E7605781",

    // Editor Groups & Tabs.
    "editorGroup.border": "#ffffffcc",
    "editorGroup.dropBackground": "#ffffffcc",

    // The editorGroupHeader.
    "editorGroupHeader.noTabsBackground": "#111111",
    "editorGroupHeader.tabsBackground": "#111111",
    "editorGroupHeader.tabsBorder": "#111111",

    // The tabs
    "tab.activeBackground": "#111111",
    "tab.activeForeground": "#FFFFFF",
    "tab.border": "#111111",
    "tab.activeBorder": "#FDE806",
    "tab.activeBorderTop": "#F06BAA",

    "tab.inactiveBackground": "#111111",
    "tab.inactiveForeground": "#ffffffcc",
    "tab.unfocusedActiveForeground": "#ffffffcc",
    "tab.unfocusedInactiveForeground": "#ffffffcc",
    "tab.activeModifiedBorder": "#ffffffcc",
    "tab.inactiveModifiedBorder": "#ffffffcc",
    "tab.unfocusedActiveModifiedBorder": "#ffffffcc",
    "tab.unfocusedInactiveModifiedBorder": "#ffffffcc",

    // The Editor widget is shown in front of the editor content. Examples are the Find/Replace dialog, the suggestion widget, and the editor hover.
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_editor-widget-colors
    "editorWidget.background": "#111111",
    "editorWidget.border": "#F06BAA",
    "editorWidget.foreground": "#ffffffcc",
    "editorWidget.resizeBorder": "#F06BAA",
    "editorSuggestWidget.background": "#111111",
    "editorSuggestWidget.border": "#00000000",
    "editorSuggestWidget.foreground": "#ffffffcc",
    "editorSuggestWidget.highlightForeground": "#FDE806",
    "editorSuggestWidget.selectedBackground": "#FDE80633",
    "editorHoverWidget.foreground": "#ffffffcc",
    "editorHoverWidget.background": "#111111",
    "editorHoverWidget.border": "#dddddd33",

    // Debug.
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_debug
    "debugToolBar.background": "#111111",
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_editor-widget-colors
    "debugExceptionWidget.background": "#E76057f5",
    "debugExceptionWidget.border": "#E76057f5",

    // Debug Icons.
    "debugIcon.startForeground": "#FDE806",
    "debugIcon.continueForeground": "#FDE806",
    "debugIcon.disconnectForeground": "#FDE806",
    "debugIcon.pauseForeground": "#FDE806",
    "debugIcon.restartForeground": "#77E8B9",
    "debugIcon.stepBackForeground": "#FDE806",
    "debugIcon.stepIntoForeground": "#FDE806",
    "debugIcon.stepOutForeground": "#FDE806",
    "debugIcon.stepOverForeground": "#FDE806",
    "debugIcon.stopForeground": "#E76057",

    // The editor marker view shows when navigating to errors and warnings in the editor (Go to Next Error or Warning command).
    "editorMarkerNavigation.background": "#3B536433",
    "editorMarkerNavigationError.background": "#E76057f5",
    "editorMarkerNavigationWarning.background": "#FDE806",

    // To see the editor white spaces, enable Toggle Render Whitespace.
    // "editorWhitespace.foreground": "#E76057f5",
    // "errorForeground": "#E76057f5",

    // Extensions.
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_extensions
    "extensionButton.background": "#FDE806",
    "extensionButton.foreground": "#111111", // reload required
    "extensionButton.prominentForeground": "#111111", // install button
    "extensionIcon.starForeground": "#F06BAA",
    "extensionIcon.verifiedForeground": "#F06BAA",

    // Input Control.
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_input-control
    "input.background": "#111111",
    // "input.border": "#5DD8E640",
    "input.foreground": "#ffffffcc",
    "input.placeholderForeground": "#808080",
    "inputOption.activeBorder": "#5DD8E6",
    "inputValidation.errorBackground": "#111111",
    "inputValidation.errorBorder": "#E7605750",
    "inputValidation.infoBackground": "#111111",
    "inputValidation.infoBorder": "#5DD8E650",
    "inputValidation.warningBackground": "#111111",
    "inputValidation.warningBorder": "#E7605750",
    "inputOption.activeBackground": "#111111",

    // Lists and Trees.
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_lists-and-trees
    // "list.dropBackground": "#ffffffcc",
    "list.activeSelectionBackground": "#111111",
    "list.activeSelectionForeground": "#5DD8E6",
    "list.activeSelectionIconForeground": "#ffffffcc",
    "list.focusBackground": "#111111",
    "list.focusForeground": "#F06BAA",
    "list.highlightForeground": "#ffffffcc",
    "list.hoverBackground": "#111111",
    "list.hoverForeground": "#ffffffcc",
    "list.inactiveSelectionBackground": "#111111",
    "list.inactiveSelectionForeground": "#5DD8E6",
    "tree.indentGuidesStroke": "#ffffff20",
    "tree.inactiveIndentGuidesStroke": "#111111",
    // "tree.tableColumnsBorder": "#ffffffcc",

    // Merge Conflicts.
    "merge.border": "#FFFFFF00",
    "merge.commonContentBackground": "#3cff0081",
    "merge.commonHeaderBackground": "#3cff0081",
    "merge.currentContentBackground": "#3ad90093",
    "merge.currentHeaderBackground": "#3ad90093",
    // "merge.incomingContentBackground": "#ff730086",
    // "merge.incomingHeaderBackground": "#ff730086",

    // Notification Colors.
    "notificationCenter.border": "#5DD8E6", // Notification Center border color.
    "notificationCenterHeader.foreground": "#ffffffcc", // Notification Center header foreground color.
    "notificationCenterHeader.background": "#111111", // Notification Center header background color.
    "notificationToast.border": "#5DD8E6", // Notification toast border color.
    "notifications.foreground": "#ffffffcc", // Notification foreground color.
    "notifications.background": "#111111", // Notification background color.
    "notifications.border": "#5DD8E6", // Notification border color separating from other notifications in the Notification Center.
    "notificationLink.foreground": "#ffffffcc", // Notification links foreground color.

    // Panel.
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_panel-colors
    "panel.background": "#111111",
    "panel.border": "#ffffff05",
    "panelTitle.activeBorder": "#FDE806",
    "panelTitle.activeForeground": "#5DD8E6",
    "panelTitle.inactiveForeground": "#FFFFFF",

    // Integrated Terminal Colors.
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_integrated-terminal-colors
    "terminal.background": "#111111",
    "terminal.foreground": "#ffffffcc",
    "terminal.border": "#ffffff10",
    // "terminal.selectionBackground": "#111111",
    "terminalCursor.background": "#F06BAA",
    "terminalCursor.foreground": "#FDE806",
    "terminal.tab.activeBorder": "#FDE806",

    // ANSI colors
    "terminal.ansiBrightBlack": "#6272A4",
    "terminal.ansiBrightRed": "#FF6E6E",
    "terminal.ansiBrightGreen": "#69FF94",
    "terminal.ansiBrightYellow": "#FFFFA5",
    "terminal.ansiBrightBlue": "#D6ACFF",
    "terminal.ansiBrightMagenta": "#FF92DF",
    "terminal.ansiBrightCyan": "#A4FFFF",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminal.ansiBlack": "#21222C",
    "terminal.ansiRed": "#FF5555",
    "terminal.ansiGreen": "#50FA7B",
    "terminal.ansiYellow": "#F1FA8C",
    "terminal.ansiBlue": "#BD93F9",
    "terminal.ansiMagenta": "#FF79C6",
    "terminal.ansiCyan": "#8BE9FD",
    "terminal.ansiWhite": "#F8F8F2",

    // Peek View Colors.
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_peek-view-colors
    "peekView.border": "#5DD8E6",
    "peekViewEditor.background": "#111111",
    "peekViewEditor.matchHighlightBackground": "#FDE80630",
    "peekViewEditorGutter.background": "#111111",
    "peekViewResult.background": "#111111",
    "peekViewResult.fileForeground": "#FFFFFF",
    "peekViewResult.lineForeground": "#FFFFFF",
    "peekViewResult.matchHighlightBackground": "#FDE80630",
    "peekViewResult.selectionBackground": "#FDE80640",
    "peekViewResult.selectionForeground": "#FFFFFF",
    "peekViewTitle.background": "#111111",
    "peekViewTitleDescription.foreground": "#FFFFFF",
    "peekViewTitleLabel.foreground": "#FDE806",

    // Quick Picker.
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_quick-picker
    "pickerGroup.border": "#111111",
    "pickerGroup.foreground": "#FDE806",

    // Progress Bar.
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_progress-bar
    "progressBar.background": "#FDE806",

    // Scroll Bar Control.
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_scroll-bar-control
    "scrollbar.shadow": "#FDE80681",
    "scrollbarSlider.background": "#F06BAA80",
    "scrollbarSlider.hoverBackground": "#5DD8E6",
    "scrollbarSlider.activeBackground": "#FDE806",

    // Title bar (macOS)
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_title-bar-colors
    "titleBar.activeBackground": "#111111",
    "titleBar.activeForeground": "#FFFFFF",
    "titleBar.inactiveBackground": "#111111",
    "titleBar.inactiveForeground": "#FFFFFF",
    // "titleBar.border": "#FFFFFF",

    // Status Bar Colors.
    // The Status Bar is shown in the bottom of the workbench.
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_status-bar-colors
    "statusBar.border": "#111111",
    "statusBar.background": "#F06BAA",
    "statusBar.foreground": "#111111",
    "statusBar.debuggingBackground": "#5DD8E6",
    "statusBar.debuggingForeground": "#111111",
    "statusBar.noFolderBackground": "#F06BAA",
    "statusBar.noFolderForeground": "#111111",
    "statusBarItem.activeBackground": "#5DD8E6",
    "statusBarItem.hoverBackground": "#5DD8E6",
    "statusBarItem.prominentBackground": "#5DD8E6",
    "statusBarItem.prominentHoverBackground": "#5DD8E6",
    "statusBarItem.remoteBackground": "#F06BAA",

    // Git VS Code theme Colors used for file labels and the SCM viewlet.
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_git-colors
    "gitDecoration.modifiedResourceForeground": "#FDE806", // Color for modified git resources.
    "gitDecoration.deletedResourceForeground": "#EC3A37F5", // Color for deleted git resources.
    "gitDecoration.untrackedResourceForeground": "#3AD900", // Color for untracked git resources.
    "gitDecoration.ignoredResourceForeground": "#3cff0081", // Color for ignored git resources.
    "gitDecoration.conflictingResourceForeground": "#FF7200", // Color for conflicting git resources.

    // Text Colors — Colors inside a text document, such as the welcome page.
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_text-colors
    "textBlockQuote.background": "#111111",
    "textBlockQuote.border": "#5DD8E6",
    "textCodeBlock.background": "#111111",
    "textLink.activeForeground": "#FDE806",
    "textLink.foreground": "#5DD8E6",
    "textPreformat.foreground": "#FDE806",
    "textSeparator.foreground": "#3cff00",
    "walkThrough.embeddedEditorBackground": "#111111",

    // Breadcrumbs.
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_breadcrumbs
    "breadcrumb.foreground": "#FFFFFF",
    "breadcrumb.background": "#111111",
    "breadcrumb.focusForeground": "#F06BAA", // Color of focused breadcrumb items.
    "breadcrumb.activeSelectionForeground": "#F06BAA", // Color of selected breadcrumb items.
    "breadcrumbPicker.background": "#111111", // Background color of breadcrumb item picker.

    // Settings Editor.
    // https://code.visualstudio.com/docs/getstarted/theme-color-reference#_settings-editor-colors
    "settings.headerForeground": "#F06BAA",
    "settings.checkboxForeground": "#FDE806",
    "settings.checkboxBackground": "#111111",
    "settings.checkboxBorder": "#FDE806",
    "settings.dropdownBackground": "#111111",
    "settings.dropdownBorder": "#FDE806",
    "settings.dropdownForeground": "#E5E4FB",
    "settings.dropdownListBorder": "#FDE806",
    "settings.textInputBackground": "#111111",
    "settings.textInputForeground": "#E5E4FB",
    "settings.textInputBorder": "#FDE806",
    "settings.numberInputBackground": "#111111",
    "settings.numberInputForeground": "#E5E4FB",
    "settings.numberInputBorder": "#FDE806",
    "settings.modifiedItemIndicator": "#FDE806",
    // "settings.focusedRowBackground": "#111111",
    // "settings.focusedRowBorder": "#FDE806",

    // Menu Bar Items
    "menu.foreground": "#FFFFFF",
    "menu.separatorBackground": "#FDE806",
    "menu.selectionForeground": "#FDE806",

    // Tab stops.
    "editor.snippetTabstopHighlightBackground": "#6943ff62",
    "editor.snippetTabstopHighlightBorder": "#6943ff62",
    "editor.snippetFinalTabstopHighlightBackground": "#6943ff62",
    "editor.snippetFinalTabstopHighlightBorder": "#6943ff62",

    // Tree List Filter Colors.
    "listFilterWidget.background": "#111111",
    "listFilterWidget.outline": "#3cff00",
    "listFilterWidget.noMatchesOutline": "#EC3A37F5",

    // Border color of active sashes.
    "sash.hoverBorder": "#FDE806"
  },

  // ~~~~~~ SYNTAX HIGHLIGHTING ~~~~~~ //
  "semanticTokenColors": {
    "variable.default_library": {
      "foreground": "#5DD8E6",
      "fontStyle": "bold italic"
    },
    "namespace": {
      "foreground": "#F06BAA",
      "fontStyle": "bold"
    }
  },
  "tokenColors": [
    {
      "name": "[COMMENTS] — The main comments color",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.line.number-sign.ruby"
      ],
      "settings": {
        "foreground": "#ffffff70",
        "fontStyle": "italic"
      }
    },
    {
      "name": "[Entity] — The main Entity color",
      "scope": "entity",
      "settings": {
        "foreground": "#FDE806"
      }
    },
    {
      "name": "[Constant] — The main constants color",
      "scope": "constant",
      "settings": {
        "foreground": "#F06BAA"
      }
    },
    {
      "name": "[Keyword] — The main color for Keyword",
      "scope": ["keyword, storage.type.class.js"],
      "settings": {
        "foreground": "#FDE806"
      }
    },
    {
      "name": "[Meta] — The main color for Meta",
      "scope": "meta",
      "settings": {
        "foreground": "#5DD8E6"
      }
    },
    {
      "name": "[invalid] — The main color for invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#E76057"
      }
    },
    {
      "name": "[Meta Brace] — The main color for Meta Brace",
      "scope": [
        "meta.brace",
        "punctuation.definition.parameters.begin.js",
        "punctuation.definition.parameters.end.js"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "[Punctuation] — The main color for Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "[Punctuation] — The main color for Punctuation",
      "scope": ["punctuation", "text.html.markdown"],
      "settings": {
        "foreground": "#F06BAA"
      }
    },
    {
      "name": "[Punctuation] — Color for Punctuation Parameters",
      "scope": "punctuation.definition.parameters",
      "settings": {
        "foreground": "#FDE806"
      }
    },
    {
      "name": "[Punctuation] — Color for Punctuation Template Expression",
      "scope": "punctuation.definition.template-expression",
      "settings": {
        "foreground": "#FDE806"
      }
    },
    {
      "name": "[Storage] — The main color for Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#FDE806"
      }
    },
    {
      "name": "[Storage] — The color for Storage Type Arrow Function",
      "scope": "storage.type.function.arrow",
      "settings": {
        "foreground": "#FDE806"
      }
    },
    {
      "name": "[String]",
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#77E8B9"
      }
    },
    {
      "name": "[String] Template Color",
      "scope": ["string.template", "punctuation.definition.string.template"],
      "settings": {
        "foreground": "#77E8B9"
      }
    },
    {
      "name": "[Support]",
      "scope": "support",
      "settings": {
        "foreground": "#77E8B9"
      }
    },
    {
      "name": "[Support] Function Colors",
      "scope": "support.function",
      "settings": {
        "foreground": "#FDE806"
      }
    },
    {
      "name": "[Support] Variable Property DOM Colors",
      "scope": "support.variable.property.dom",
      "settings": {
        "foreground": "#5DD8E6"
      }
    },
    {
      "name": "[Variable]",
      "scope": "variable",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "[INI] - Color for Entity",
      "scope": ["source.ini entity", "meta.embedded.block.ini", "source.ini"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "[INI] - Color for Keyword",
      "scope": ["source.ini keyword", "keyword.other.definition.ini"],
      "settings": {
        "foreground": "#FDE806"
      }
    },
    {
      "name": "[INI] - Color for Punctuation Definition",
      "scope": ["source.ini punctuation.definition"],
      "settings": {
        "foreground": "#FDE806"
      }
    },
    {
      "name": "[INI] - Color for Punctuation Separator",
      "scope": [
        "source.ini punctuation.separator",
        "punctuation.separator.key-value.ini"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "[CSS] - Color for Entity",
      "scope": ["source.css entity", "source.stylus entity"],
      "settings": {
        "foreground": "#5DD8E6"
      }
    },
    {
      "name": "[CSS] - Color for Class Selector",
      "scope": "entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#F06BAA"
      }
    },
    {
